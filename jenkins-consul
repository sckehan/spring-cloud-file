pipeline {
   agent {label 'linux'}
   stages {
      
      stage('Pull Code') {
         steps {
            git 'https://github.com/sckehan/spring-cloud-examples.git'
            sh 'ls'
         }

      }
     
      stage('Build Code') {
         steps {
            sh 'mvn package'
            sh 'ls target/'
         }

      }

   }
}
